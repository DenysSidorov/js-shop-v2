                <div class="cardSection left fullWidth ">
                    <div class="container">
                        <div class="productsCardBlock">
                            <div class="productsCardBlock__navigation"></div>
                            <div class="productsCardBlock__searchCard"></div>
                            <div class="productsCardBlock__bodyCardItems">

                                <% goods.goods.forEach(function(card) { %>
                                <div class="bodyCardItems__oneCardItem">

                                    <div class="oneCardItem__headCard">
                                        <div class="oneCardItem__headCard__priceCard">
                                            <span><%= card.price %></span><span></span>&nbsp;<span>грн.</span>
                                        </div>
                                        <div class="oneCardItem__headCard__nameBrand">
                                            <%= card.name %> <%= card.model %>
                                        </div>
                                        <div class="oneCardItem__headCard__wrap-things">
                                            <!--<div class="oneCardItem__headCard__otherThings">-->
                                                <!--<i class="fa fa-heart"></i>-->
                                                <!--<span class="oneCardItem__headCard__otherThings_like"> <%= card.likes %></span>-->
                                            <!--</div>-->
                                            <div class="oneCardItem__headCard__view">
                                                <i class="fa fa-eye" aria-hidden="true"></i>
                                                <span class="oneCardItem__headCard__view_items"> <%= card.views %></span>
                                            </div>
                                        </div>
                                    </div>

                                    <a href="/<%= card._id%>" class="oneCardItem__imageBody">
                                        <img src="/img-static/<%= card.photo[0] %>" alt="">
                                    </a>

                                    <div class="oneCardItem__bottomCard">
                                        <div class="oneCardItem__bottomCard__shortText">
                                            <span class="oneCardItem__bottomCard__shortText_dots">
                                                 <%= card['desc-short'] %>
                                            </span>
                                        </div>

                                        <% if(card.isExists) { %>
                                            <div class="oneCardItem__bottomCard__status">В наличии</div>
                                        <% } else { %>
                                        <div class="oneCardItem__bottomCard__status red">Нет в наличии</div>
                                        <% } %>
                                        <div class="oneCardItem__bottomCard__buy"><span>В КОРЗИНУ</span></div>
                                    </div>
                                    <% if (card.sail) { %>
                                        <div class="oneCardItem__bottomCard_sale">
                                            <span>-<%= card.sail %>%</span>
                                        </div>
                                    <% }%>

                                    <% if (card.isNewGood) { %>
                                    <div class="oneCardItem__bottomCard_new">NEW<span></span></div>
                                    <% }%>

                                </div>
                                <% }); %>




                            </div>
                        </div>
                    </div>
